	Doi = {10.1109/SMC-IT.2014.13},
	Keywords = {aerospace computing;software prototyping;systems engineering;Scrum;XP;engineering standards;extreme programming;formal methodologies;industry globalization;mission criticality;mission-critical systems engineering;performance requirements;safety requirements;safety standards;software engineering;space industry;space-based systems engineering;Context;Industries;Mission critical systems;Organizations;Software;Standards;Systems engineering and theory;agile;mission-critical;scrum;systems development},
	Month = {Sept},
